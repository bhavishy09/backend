      <div class="users flex gap-3 flex-wrap">
        <a  class="text-blue-900 "href="/">back to home</a>
        <% users.forEach(function(users) { %>
          <div class="user w-72 p-4 bg-zinc-800 rounded-lg">
            <div class="w-full h-52 rounded-lg bg-zinc-700 overflow-hidden">
                <img class="w-full h-full object-cover object-center" src="<%= users.img %>">

            </div>
            <h3 class="text-xl mt-3 tracking-tighter"><%= users.name %></h3>
            <h5 class="text-zinc-500"><%= users.email %></h5>
    
            <div class="flex gap-3 mt-10">
              <a class="text-zinc-200" href="">Edit User</a>
              <a class="text-red-500" href="/delete/<%= users.id%>">Delete User</a>
            </div>
          </div>
        <% }) %>
      </div>
    </div>
   
    
    
    
    
    
    
    
    
    
    





